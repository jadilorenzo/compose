<div class='container'>
  <div class='documents-search-bar'>
    <form>
      <div class='material-symbols-rounded'>search</div> 
      <input placeholder='Search...'/>
    </form>
    <%= link_to "note_add", new_document_path, class:'material-symbols-rounded' %>
  </div>
  <div class='document-list'>
    <% @documents.each do |document| %>
      <div class='document-item'>
        <div class='material-symbols-rounded'>note</div>
        <%= link_to document.title, document %>
      </div>
    <% end %>
  </div>
</div>